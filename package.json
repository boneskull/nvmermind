{
  "name": "nvmermind",
  "version": "0.2.3",
  "description": "Uninstall old versions of Node.js as installed by nvm",
  "keywords": [
    "nvm",
    "node",
    "nodejs",
    "node.js",
    "uninstall",
    "clean",
    "cleanup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/boneskull/nvmermind"
  },
  "license": "Apache-2.0",
  "author": "Christopher Hiller <boneskull@boneskull.com> (https://boneskull.com/)",
  "main": "src/index.js",
  "bin": {
    "nvmermind": "./src/index.js"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "standard-version -a",
    "test": "nyc mocha \"test/**/*.spec.js\"",
    "posttest": "markdownlint-cli2 \"*.md\" && eslint ."
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.md": [
      "markdownlint-cli2-fix"
    ],
    "*.{yml,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "endOfLine": "auto",
    "singleQuote": true
  },
  "dependencies": {
    "@humanwhocodes/env": "^2.2.0",
    "ora": "^5.4.1",
    "prompts": "^2.4.1",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@types/semver": "7.3.9",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-semistandard": "15.0.1",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-promise": "5.1.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "markdownlint-cli2": "0.4.0",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "prettier": "2.3.0",
    "sinon": "11.1.2",
    "standard-version": "9.3.2",
    "unexpected": "12.0.4",
    "unexpected-sinon": "11.0.1"
  },
  "engines": {
    "node": ">=14"
  }
}
